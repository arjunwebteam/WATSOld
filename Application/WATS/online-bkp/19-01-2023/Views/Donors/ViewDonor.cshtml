@{
    ViewBag.Title = "ViewEnquiry";
    Layout = null;

    WATS.Entities.Donors objDonors = ViewBag.objDonors;
}
@{var baseurl = System.Configuration.ConfigurationManager.AppSettings["baseurl"];}
<base href="@(baseurl)" />
<link href="Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link rel="stylesheet" type="text/css" href="Content/jquery-ui.css" />
<link href="Content/admin-style.css" rel="stylesheet" media="screen">
<link href="Content/admin-comman-styles.css" rel="stylesheet" media="screen">
<link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700' rel='stylesheet' type='text/css'>

<table width="574" cellspacing="0" cellpadding="0" border="0" align="center" class="main-text">
    <tbody>

        <tr>
            <td>
                <div id="messages"></div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="custom-adn grayBackground nobL nobR">
                    <div class="ui-accordion ui-widget ui-helper-reset" role="tablist">
                        <h3 class="ui-accordion-header ui-helper-reset ui-state-default ui-accordion-header-active ui-state-active ui-corner-top ui-accordion-icons" role="tab" id="ui-accordion-2-header-0" aria-controls="ui-accordion-2-panel-0" aria-selected="true" tabindex="0"><span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-s"></span><b>Donor Details</b></h3>
                        <div class="form-horizontal ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom ui-accordion-content-active" style="display: block;" id="ui-accordion-2-panel-0" aria-labelledby="ui-accordion-2-header-0" role="tabpanel" aria-expanded="true" aria-hidden="false">
                            <div class="filedsetInner clearfix">

                                <table width="100%" cellspacing="0" cellpadding="6" border="0" class="mgrey2">
                                    <tbody>
                                        <tr>
                                            <td width="360"><strong>First Name : </strong></td>
                                            <td width="724" class="t-t-c">@objDonors.FirstName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Last Name : </strong></td>
                                            <td>@objDonors.LastName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Email : </strong></td>
                                            <td>@objDonors.Email</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Phone No : </strong></td>
                                            <td>@(objDonors.PhoneNo == "" ? "N/A" : objDonors.PhoneNo)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Amount : </strong></td>
                                            <td>@objDonors.Amount</td>
                                        </tr>
                                        <tr>
                                            <td><strong>TransactionId : </strong></td>
                                            <td>@(objDonors.TransactionId == "" ? "N/A" : objDonors.TransactionId)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Payment Status : </strong></td>
                                            <td>@(objDonors.PaymentStatus == "" ? "N/A" : objDonors.PaymentStatus)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Payment Method : </strong></td>
                                            <td>@(objDonors.PaymentMethod == "" ? "N/A" : objDonors.PaymentMethod)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Order Date : </strong></td>
                                            <td>@(objDonors.OrderDate == DateTime.MinValue ? "N/A" : objDonors.OrderDate.ToString("MM-dd-yyyy"))</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Address : </strong></td>
                                            <td>@(objDonors.Address == "" ? "N/A" : objDonors.Address)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Donation Program : </strong></td>
                                            <td>@(objDonors.DonationProgram == "" ? "N/A" : objDonors.DonationProgram)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Donation Cause : </strong></td>
                                            <td>@(objDonors.DonationCause == "" ? "N/A" : objDonors.DonationCause)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Payment By : </strong></td>
                                            <td>@(objDonors.PaymentBy == "" ? "N/A" : objDonors.PaymentBy)</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </td>
        </tr>
    </tbody>
</table>
