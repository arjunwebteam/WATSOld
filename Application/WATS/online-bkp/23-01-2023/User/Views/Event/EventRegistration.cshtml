    function verifymember() {
        if (validateverify()) {
            for (j = 1; j <= '@typescount'; j++) {
                if (('@Memberdetails.MemberId' != 0) && ('@Memberdetails.MembershipType' == "Life Membership" || '@Memberdetails.MembershipType' == "Life Membership Single")) {
                    $('#loginERShow').show();
                    $('#loginERHide').hide();
                }
                else {
                    var SpouseCell = $('#MemberId1').val();
                    var returnval = null;

                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("CheckMemberAvailability", "Event")', // the URL of the controller action method
                        data: { SpouseCell: SpouseCell }, // optional data
                        datatype: "JSON",
                        async: false,
                        success: function (result) {
                            if (result.data == true) {
                                $('#memberalert').hide();
                                $('#loginERHide').hide();
                                $('#loginERShow').show();

                                window.location.href = "@Request.Url.AbsoluteUri" + '&MId=' + result.mid + '&type=' + result.type;
                            }
                            else {
                                $('#memberalert').show();
                                $('#loginERShow').hide();
                                $('#loginERHide').show();
                            }
                        }
                    });
                    return returnval;
                }
            }
        }
                            }
