@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    var objEvents = (WATS.Entities.Events)ViewBag.objEvents;
    var Memberdetails = (WATS.Entities.Members)ViewBag.Memberdetails;
    var desc = "";
    var innerpage = (WATS.Entities.InnerPages)ViewBag.InnerPage;
    if (innerpage != null)
    {
        desc = innerpage.Description;
    }

    List<WATS.Entities.EventRegistrationTypes> lstEventRegistrationTypes = ViewBag.lstEventRegistrationTypes;

    decimal MemberTotal = (objEvents.IsChild == true ? objEvents.ChildAmount : 0) + (objEvents.IsAdult == true ? objEvents.AdultAmount : 0) + (objEvents.IsCouple == true ? objEvents.CoupleAmount : 0) + (objEvents.IsVIPChild == true ? objEvents.VIPAmount : 0) + (objEvents.IsFamily == true ? objEvents.FamilyAmount : 0);
    decimal NonMemberTotal = (objEvents.IsChild == true ? objEvents.NonChildAmount : 0) + (objEvents.IsAdult == true ? objEvents.NonAdultAmount : 0) + (objEvents.IsCouple == true ? objEvents.NonCoupleAmount : 0) + (objEvents.IsVIPChild == true ? objEvents.VIPAmount : 0) + (objEvents.IsFamily == true ? objEvents.NonFamilyAmount : 0);

    Int64 typescount = (Memberdetails.MemberId != 0 && Memberdetails.MembershipType == "Life Membership" ? lstEventRegistrationTypes.Count(x => x.RegistrationTitle.Contains("Life")) : lstEventRegistrationTypes.Count(x => !x.RegistrationTitle.Contains("Life")));

    if (ViewBag.MId != 0)
    {
        Memberdetails.MemberId = ViewBag.MId;
        Memberdetails.MembershipType = "Life Membership";
        Memberdetails.MembershipTypeId = 1;
    }

}

