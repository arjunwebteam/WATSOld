<script type="text/javascript">
    function forgotpopup() {
        $('#forgotmemberid').dialog('open');
    }
                        function getmemberid() {
                            $('#getyourmember').show();
    }

    function Forgotmember() {
        if (validateforgot()) {
            var Email = $('#EmailId1').val();

            $.ajax({
                type: "POST",
                url: '@Url.Action("ForgottenMemberId", "Event")', // the URL of the controller action method
                data: { MemberEmail: Email }, // optional data
                datatype: "JSON",
                async: false,
                success: function (result) {
                    if (result.ok) {
                        $('#memforgottenalert').hide();
                        $('#divmessage').html(result.data);
                        $('#alertfail').html('');
                        $('#showforgot').hide();
                        $('#getyourmember').hide();
                        $('#EmailId1').val('');
                        $('#forgotmemberid').dialog('close');

                    }
                    else {
                        $('#memforgottenalert').show();
                        $('#alertfail').html(result.data);
                        $('#alert').html('');
                        $('#showforgot').show();
                        
                        $('#getyourmember').show();
                    }
                }
            });
        } 
        
    }

    function validateforgot() {
        var return_val = true;

        if ($('#EmailId1').val() == "") { $('#EmailId1').focus().addClass("validate"); document.getElementsByName('Email2')[0].placeholder = 'Required'; return_val = false; } else {
            $('#EmailId1').focus().removeClass("validate");
            if (!Common.CheckEmail($('#EmailId1').val())) { $('#EmailId1').val(''); document.getElementsByName('Email2')[0].placeholder = 'Invalid email address'; $('#EmailId1').focus().addClass("validate"); return_val = false; } else { $('#EmailId1').removeClass("validate"); }
        }


        
        return return_val;
        
    }

    


</script>

