                    <span class="dis-in l-p15 dk-t">@objMemberDetails.SpouseCell  </span>
                </article>
            </article>
            <article class="clearfix box-row b-p5">
                <article class="box-5">
                    Membership Type<span class="dis-in pull-right">:</span>
                </article>
                <article class="box-7">
                    <span class="dis-in l-p15 dk-t">@objMemberDetails.MembershipType </span>
                </article>
            </article>

            <article class="clearfix box-row b-p5">
                <article class="box-5">
                    ExpiryDate<span class="dis-in pull-right">:</span>
                </article>
                <article class="box-7">
                    <span class="dis-in l-p15 dk-t">@(objMemberDetails.ExpiryDate == DateTime.MinValue ? (objMemberDetails.objMembershipOrder.PaymentStatus == "Completed" ? "Life Time" : "N/A") : objMemberDetails.ExpiryDate.ToString("MM-dd-yyyy"))</span>
                </article>
            </article>
            <article class="clearfix box-row b-p5">
                <article class="box-5">
