@{
    ViewBag.Title = "ViewCommittee";
    Layout = "~/Views/Shared/_MainLayout.cshtml";

    WATS.Entities.Committees objCommittees = ViewBag.objDetails;
}

<!-- breadcumb -->
<div class="clearfix breadcrumb-main"><ul class="breadcrumb  ">
    
    <li><a href="@Url.Action("Index", "InnerPages")">Web Site</a> <span class="divider">/</span></li>
    <li><a href="@Url.Action("Index", "Committees")">Committees</a> <span class="divider">/</span></li>
    <li class="active">View Committee Members</li>
    <li class="fR sepV_c"><i class="icon-globe"></i><a href="@System.Configuration.ConfigurationManager.AppSettings["usersiteurl"]" target="_blank">View Mainsite</a></li>
</ul></div>
<!-- end breadcumb -->

<div id="maincontainer">
    <div class="mainContent">
        <!--- TOPEBAR [start] ---->
        @Html.Partial("MainMenu", "SuperAdmin")
        <!--- TOPEBAR [end] ---->

        <!--- SIDEBAR [start] ---->
        @Html.Partial("LeftMenu")
        <!--- SIDEBAR [en] ---->
        <div class="pageHeadingBlock ">
            <div class="grayBackground">
                <div class="fR t-r spcT_c">
                    <a class="clearfix" onclick="history.back();">←back to Committee members</a>
                </div>
                <h3 class="title">Committee Members </h3>
                <div class="sectionTabNav">
                    <div class="btn-group">
                        <a class="btn" href="@Url.Action("Index", "Committees")">Committee Members</a>
                        <a class="btn" href="@Url.Action("Index", "CommitteeMaster")">Committees</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix sepH_b"></div>

        <div id="divmessage">
            @if (TempData["message"] != null)
            { @Html.Raw(TempData["message"].ToString())}

            @if (ViewBag.message != null)
            { @Html.Raw(ViewBag.message.ToString())}
        </div>

        <div class="l-r-p20">
            <h4 class="bor0 black-t grayBackground "><b>Member Details</b></h4>
            <div class="filedsetInner clearfix">
                <div class="row-fluid">
                    <div class="span6">
                        <div class="clearfix rp10">
                            <div class="row-fluid">
                                <div class="span5">
                                    <label>Name : </label>
                                </div>
                                <div class="span7 t-t-c">
                                    @objCommittees.Name
                                </div>
                            </div>
                            <div class="row-fluid">
                                <div class="span5">
                                    <label>Phone No :</label>
                                </div>
                                <div class="span7">
                                    @objCommittees.PhoneNo
                                </div>
                            </div>
                            <div class="row-fluid">
                                <div class="span5">
                                    <label>State : </label>
                                </div>
                                <div class="span7">
                                    @objCommittees.State
                                </div>
                            </div>
                            <div class="row-fluid">
                                <div class="span5">
                                    <label>Address :</label>
                                </div>
                                <div class="span7">
                                    @objCommittees.Address
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="span6">
                        <div class="clearfix">
                            <div class="row-fluid">
                                <div class="span5">
                                    <label>Email : </label>
                                </div>
                                <div class="span7">
                                    @objCommittees.Email
                                </div>
                            </div>
                            <div class="row-fluid">
                                <div class="span5">
                                    <label>City :</label>
                                </div>
                                <div class="span7">
                                    @objCommittees.City
                                </div>
                            </div>
                            <div class="row-fluid">
                                <div class="span5">
                                    <label>Display Order :</label>
                                </div>
                                <div class="span7">
                                    @objCommittees.DisplayOrder
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="l-r-p20">
            <h4 class="bor0 black-t grayBackground "><b>Member Description</b></h4>
            <div class="filedsetInner clearfix">
                @Html.Raw(objCommittees.Description)
            </div>
        </div>

    </div>
</div>
