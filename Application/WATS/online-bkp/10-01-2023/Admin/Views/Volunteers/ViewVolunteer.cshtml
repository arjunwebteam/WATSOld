@{
    ViewBag.Title = "ViewVolunteer";
    Layout = null;

    WATS.Entities.Volunteers objVolunteers = ViewBag.objVolunteers;
}
@{var baseurl = System.Configuration.ConfigurationManager.AppSettings["baseurl"];}
<base href="@(baseurl)" />
<link href="Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link rel="stylesheet" type="text/css" href="Content/jquery-ui.css" />
<link href="Content/admin-style.css" rel="stylesheet" media="screen">
<link href="Content/admin-comman-styles.css" rel="stylesheet" media="screen">
<link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700' rel='stylesheet' type='text/css'>
<script src="Scripts/jquery-1.8.3.js" type="text/javascript"></script>
<script src="Scripts/jquery-ui-1.9.2.custom.min.js" type="text/javascript"></script>
<script src="Scripts/jquery.ui.widget.js"></script>
<script src="Scripts/jquery.ui.accordion.js"></script>
<script src="Scripts/delete_item.js" type="text/javascript"></script>
<script type="text/javascript" src="Scripts/jquery-ui-timepicker-addon.js"></script>
<script type="text/javascript" src="Scripts/jquery-ui-sliderAccess.js"></script>

<table width="784" cellspacing="0" cellpadding="0" border="0" align="center" class="main-text">
    <tbody>
        <tr>
            <td>
                <div id="messages"></div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="custom-adn grayBackground nobL nobR">
                    <div class="ui-accordion ui-widget ui-helper-reset" role="tablist">
                        <h3 class="ui-accordion-header ui-helper-reset ui-state-default ui-accordion-header-active ui-state-active ui-corner-top ui-accordion-icons" role="tab" id="ui-accordion-2-header-0" aria-controls="ui-accordion-2-panel-0" aria-selected="true" tabindex="0"><span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-s"></span><b>Volunteer Details</b></h3>
                        <div class="form-horizontal ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom ui-accordion-content-active" style="display: block;" id="ui-accordion-2-panel-0" aria-labelledby="ui-accordion-2-header-0" role="tabpanel" aria-expanded="true" aria-hidden="false">
                            <div class="filedsetInner clearfix">
                                <table width="100%" cellspacing="0" cellpadding="6" border="0" class="mgrey2">
                                    <tbody>
                                        <tr>
                                            <td width="300"><strong>First Name : </strong></td>
                                            <td width="724">@objVolunteers.FirstName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Last Name : </strong></td>
                                            <td>@objVolunteers.LastName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Email : </strong></td>
                                            <td>@objVolunteers.Email</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Phone No : </strong></td>
                                            <td>@(objVolunteers.PhoneNo==null?"N/A":objVolunteers.PhoneNo)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Address : </strong></td>
                                            <td>@(objVolunteers.Address==null?"N/A":objVolunteers.Address)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Comment : </strong></td>
                                        </tr>
                                        <tr>
                                            <td>@(objVolunteers.Comment==null?"N/A":objVolunteers.Comment)</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </td>
          
        </tr>
        <tr>
            <td>
                <div class="custom-adn grayBackground nobL nobR">
                    <div class="ui-accordion ui-widget ui-helper-reset" role="tablist">
                        <h3 class="ui-accordion-header ui-helper-reset ui-state-default ui-accordion-header-active ui-state-active ui-corner-top ui-accordion-icons" role="tab" id="ui-accordion-2-header-0" aria-controls="ui-accordion-2-panel-0" aria-selected="true" tabindex="0"><span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-s"></span><b>Youth/ Adult Details</b></h3>
                        <div class="form-horizontal ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom ui-accordion-content-active" style="display: block;" id="ui-accordion-2-panel-0" aria-labelledby="ui-accordion-2-header-0" role="tabpanel" aria-expanded="true" aria-hidden="false">
                            <div class="filedsetInner clearfix">
                                <table width="100%" cellspacing="0" cellpadding="6" border="0" class="mgrey2">
                                    <tbody>
                                        <tr>
                                            <td width="300"><strong>School Name : </strong></td>
                                            <td width="724">@objVolunteers.SchoolName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Grade in school : </strong></td>
                                            <td>@objVolunteers.Gradeinschool</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Company Name: </strong></td>
                                            <td>@objVolunteers.CompanyName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Notes : </strong></td>
                                            <td>@objVolunteers.Notes</td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </td>
        </tr>
    </tbody>
</table>
