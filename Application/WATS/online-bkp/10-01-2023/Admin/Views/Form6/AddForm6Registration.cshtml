                                                    <option value="">-- Select Method --</option>
                                                    @if (lstPaymentMethod != null && lstPaymentMethod.Count != 0)
                                                    {
                                                        foreach (var item in lstPaymentMethod)
                                                        {
                                                            <option value="@item.PaymentMethodId">@item.PaymentMethod</option>
                                                        }
                                                    }
                                                </select>
