                            <div class="filedsetInner clearfix">

                                <table width="100%" cellspacing="0" cellpadding="6" border="0" class="mgrey2">
                                    <tbody>
                                        <tr>
                                            <td width="270"><strong>Address : </strong></td>
                                            <td width="724">@objMemberDetails.UserName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>State : </strong></td>
                                            <td>@(objMemberDetails.State == null ? "N/A" : objMemberDetails.State)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>City : </strong></td>
                                            <td>@(objMemberDetails.City == null ? "N/A" : objMemberDetails.City)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Zip Code : </strong></td>
                                            <td>@(objMemberDetails.ZipCode == null ? "N/A" : objMemberDetails.ZipCode)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Home Phone : </strong></td>
                                            <td>@(objMemberDetails.HomePhone == null ? "N/A" : objMemberDetails.HomePhone)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Comments : </strong></td>
                                            <td>@(objMemberDetails.Comments == null ? "N/A" : objMemberDetails.Comments)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Mobile Phone : </strong></td>
                                            <td>@(objMemberDetails.MobilePhone == null ? "N/A" : objMemberDetails.MobilePhone)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Is Volunteer : </strong></td>
                                            <td>@objMemberDetails.IsVolunteer</td>
                                        </tr>
                                        <tr>
