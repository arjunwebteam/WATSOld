@{
    ViewBag.Title = "ViewEnquiry";
    Layout = null;

    WATS.Entities.AdvertiseWithUs objAdvertiseWithUs = ViewBag.objAdvertiseWithUs;
}
@{var baseurl = System.Configuration.ConfigurationManager.AppSettings["baseurl"];}
<base href="@(baseurl)" />
<link href="Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link rel="stylesheet" type="text/css" href="Content/jquery-ui.css" />
<link href="Content/admin-style.css" rel="stylesheet" media="screen">
<link href="Content/admin-comman-styles.css" rel="stylesheet" media="screen">
<link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700' rel='stylesheet' type='text/css'>
<table width="574" cellspacing="0" cellpadding="0" border="0" align="center" class="main-text">
    <tbody>
        <tr>
            <td>
                <div class="custom-adn grayBackground nobL nobR">
                    <div class="ui-accordion ui-widget ui-helper-reset" role="tablist">
                        <h3 class="ui-accordion-header ui-helper-reset ui-state-default ui-accordion-header-active ui-state-active ui-corner-top ui-accordion-icons" role="tab" id="ui-accordion-2-header-0" aria-controls="ui-accordion-2-panel-0" aria-selected="true" tabindex="0"><span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-s"></span><b>Advertise With Us Details</b></h3>
                        <div class="form-horizontal ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom ui-accordion-content-active" style="display: block;" id="ui-accordion-2-panel-0" aria-labelledby="ui-accordion-2-header-0" role="tabpanel" aria-expanded="true" aria-hidden="false">
                            <div class="filedsetInner clearfix">
                                <table width="100%" cellspacing="0" cellpadding="6" border="0" class="mgrey2">
                                    <tbody>
                                        <tr>
                                            <td width="300"><strong>First Name : </strong></td>
                                            <td width="724">@objAdvertiseWithUs.FirstName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Last Name : </strong></td>
                                            <td>@objAdvertiseWithUs.LastName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Company Name : </strong></td>
                                            <td>@objAdvertiseWithUs.CompanyName</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Email : </strong></td>
                                            <td>@objAdvertiseWithUs.Email</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Phone No : </strong></td>
                                            <td>@(objAdvertiseWithUs.PhoneNo == "" ? "N/A" : objAdvertiseWithUs.PhoneNo)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Amount : </strong></td>
                                            <td>@(objAdvertiseWithUs.Amount)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>TransactionId : </strong></td>
                                            <td>@(objAdvertiseWithUs.TransactionId == "" ? "N/A" : objAdvertiseWithUs.TransactionId)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Payment Status : </strong></td>
                                            <td>@(objAdvertiseWithUs.PaymentStatus == "" ? "N/A" : objAdvertiseWithUs.PaymentStatus)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Payment Method : </strong></td>
                                            <td>@(objAdvertiseWithUs.PaymentMethod == "" ? "N/A" : objAdvertiseWithUs.PaymentMethod)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Payment Date : </strong></td>
                                            <td>@(objAdvertiseWithUs.PaymentDate == DateTime.MinValue ? "N/A" : objAdvertiseWithUs.PaymentDate.ToString("MM-dd-yyyy"))</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Address : </strong></td>
                                            <td>@(objAdvertiseWithUs.Address == "" ? "N/A" : objAdvertiseWithUs.Address)</td>
                                        </tr>
                                        <tr>
                                            <td><strong>Comment : </strong></td>
                                        </tr>
                                        <tr>
                                            <td>@(objAdvertiseWithUs.Comment == "" ? "N/A" : objAdvertiseWithUs.Comment)</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </td>
        </tr>
    </tbody>
</table>
