            if ($('#FatherEmail').val() == "") { $('#spanFatherEmail').html('Required.'); $('#FatherEmail').focus().addClass(""); return false; } else { $('#spanFatherEmail').html(''); $('#FatherEmail').focus().removeClass(""); }
            if ($('#FatherPhoneNo').val() == "") { $('#spanFatherPhoneNo').html('Required.'); $('#FatherPhoneNo').focus().addClass(""); return false; } else { $('#spanFatherPhoneNo').html(''); $('#FatherPhoneNo').focus().removeClass(""); }
            if ($('#MotherFirstName').val() == "") { $('#spanMotherFirstName').html('Required.'); $('#MotherFirstName').focus().addClass(""); return false; } else { $('#spanMotherFirstName').html(''); $('#MotherFirstName').focus().removeClass(""); }
            if ($('#MotherLastName').val() == "") { $('#spanMotherLastName').html('Required.'); $('#MotherLastName').focus().addClass(""); return false; } else { $('#spanMotherLastName').html(''); $('#MotherLastName').focus().removeClass(""); }
            if ($('#MotherEmail').val() == "") { $('#spanMotherEmail').html('Required.'); $('#MotherEmail').focus().addClass(""); return false; } else { $('#spanMotherEmail').html(''); $('#MotherEmail').focus().removeClass(""); }
            if ($('#MotherPhoneNo').val() == "") { $('#spanMotherPhoneNo').html('Required.'); $('#MotherPhoneNo').focus().addClass(""); return false; } else { $('#spanMotherPhoneNo').html(''); $('#MotherPhoneNo').focus().removeClass(""); }
            if ($('#TeluguBadiCenterName').val() == "") { $('#spanTeluguBadiCenterName').html('Required.'); $('#TeluguBadiCenterName').focus().addClass(""); return false; } else { $('#spanTeluguBadiCenterName').html(''); $('#TeluguBadiCenterName').focus().removeClass(""); }

            //var rowcount = $('#hdnRowCount').val();
            //for (var i = 0; i < rowcount; i++) {
            //    if ($("#FirstName-" + i).val() != "" || $("#LastName-" + i).val() != "" || $("#Age-" + i).val() != "" || $("#Relationship-" + i).val() != "") {
            //        if ($("#FirstName-" + i).val() == '') { alert('Please enter FirstName.'); $("#FirstName-" + i).focus(); return false; }
            //        if ($("#LastName-" + i).val() == '') { alert('Please enter LastName'); $("#LastName-" + i).focus(); return false; }
            //        if ($("#Age-" + i).val() == '') { alert('Please enter Age.'); $("#Age-" + i).focus(); return false; }
            //        if ($("#Relationship-" + i).val() == '') { alert('Please enter Relationship.'); $("#Relationship-" + i).focus(); return false; }
            //    }
            //}

            return true;
        }



        function addRow() {

            var table = document.getElementById("TeluguBadiStudentstbl");

            var rowCount = table.rows.length;
            var row = table.insertRow(rowCount);
            row.id = rowCount;

            var cell1 = row.insertCell(0);
            var element1 = document.createElement("input");
            element1.type = "text";
            element1.name = "[" + (rowCount - 1) + "].FirstName";
            element1.id = "FirstName-" + (rowCount - 1);
            element1.className = "input-block-level b-m0";
            cell1.appendChild(element1);


            var cell2 = row.insertCell(1);
            var element2 = document.createElement("input");
            element2.type = "text";
            element2.name = "[" + (rowCount - 1) + "].LastName";
            element2.id = "LastName-" + (rowCount - 1);
            element2.className = "input-block-level b-m0";
            cell2.appendChild(element2);

            var cell3 = row.insertCell(2);
            var element3 = document.createElement("input");
            element3.type = "text";
            element3.className = "input-block-level b-m0";
            element3.name = "[" + (rowCount - 1) + "].Age";
            element3.id = "Age-" + (rowCount - 1);
            cell3.appendChild(element3);

            //var cell4 = row.insertCell(3);
            //var element4 = document.createElement("input");
            //element4.type = "text";
            //element4.className = "input-block-level b-m0";
            //element4.name = "[" + (rowCount - 1) + "].LevelOfferings";
            //element4.id = "LevelOfferings-" + (rowCount - 1);
            //cell4.appendChild(element4);

            //var cell4 = row.insertCell(3);
            //var element4 = document.createElement("select");
            //element4.type = "text";
            //element4.className = "input-block-level b-m0";
            //element4.name = "[" + (rowCount - 1) + "].LevelOfferings";
            //element4.id = "LevelOfferings-" + (rowCount - 1);
            //cell4.appendChild(element4);


            var cell4 = row.insertCell(3);
            var element4 = document.createElement("select");
            element4.name = "[" + (rowCount - 1) + "].LevelOfferings";
            element4.id = "LevelOfferings-" + (rowCount - 1);
            element4.className = "input-block-level b-m0";
            element4.innerHTML = "<option value=\"\">-- Select --</option><option value=\"0\">0</option><option value=\"1\">1</option><option value=\"2\">2</option><option value=\"3\">3</option><option value=\"4\">4</option>";
            cell4.appendChild(element4);

     

            var cell5 = row.insertCell(4);
            var element5 = document.createElement("select");
            element5.name = "[" + (rowCount - 1) + "].Field1";
            element5.id = "Field1-" + (rowCount - 1);
            element5.className = "input-block-level b-m0";
            element5.innerHTML = "<option value=\"\">-- Select --</option><option value=\"Redmond\">Redmond</option><option value=\"Bothell\">Bothell</option><option value=\"Sammamish\">Sammamish</option>";
            cell5.appendChild(element5);

            document.getElementById("Age-" + (rowCount - 1)).onkeypress = function (event) { return Common.isNumberKey(event) };
            var cell6 = row.insertCell(5);
            cell6.innerHTML = '<a onclick="javascript:deleteRow(' + rowCount + ')" class="pagedelete t-p10"><i class="icon-trash"></i></a>';
            $('#hdnRowCount').val(rowCount);
        }
        function deleteRow(i) {
            try {
                var row = document.getElementById(i);
                row.parentNode.removeChild(row);
            } catch (e) {
                alert(e);
            }
        }

    </script>
<input type="hidden" value="1" id="hdnRowCount" />

