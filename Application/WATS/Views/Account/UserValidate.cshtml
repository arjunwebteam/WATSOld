@using System.Configuration
@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    @{var baseurl = ConfigurationManager.AppSettings["baseurl"];}
    <base href="@(baseurl)" />
    <title>WATS - Admin Panel</title>

    <link rel="stylesheet" type="text/css" href="Content/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="Content/jquery-ui.css" />
    <link rel="stylesheet" type="text/css" href="Content/admin-style.css" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700' rel='stylesheet' type='text/css'>
    <!-- ============== GENRAL JS [start] ========= -->
    <script type="text/javascript" src="Scripts/jquery-1.8.3.js"></script>
    <script src="Scripts/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="Scripts/Common.js" type="text/javascript"></script>
    <!-- =============GENRAL JS [end] ================ -->

    <script type="text/javascript" src="Content/bootstrap/js/bootstrap.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.closable').append('<span class="closelink" title="Close"></span>');
            $('.closelink').click(function () {
                $(this).parent().fadeOut('600', function () { $(this).remove(); });
            });
        });

    </script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="brand" href="@Url.Action("Index","Member")" target="_blank">
                    <img width="280" title="WATS" src="Content/images/logo.png" alt="WATS" /></a>
            </div>
        </div>
    </div>
    <div class="loginWrapper">
        <div class="loginContent">
            <!-- login form -->
            <div class="loginSection clearfix">
               
                <div class="clearfix"></div>
                
                 @if (ViewBag.message != null)
        {
                     <h3 class="title font16 t-c b-p40">@Html.Raw(ViewBag.message)</h3>@*
            <div id="divmessage" style="max-width:675px;margin:0 auto;" class="t-c">@Html.Raw(ViewBag.message)</div> *@
        }
                     
                <div style="clear: both"></div>
            </div>
            <!-- end login form -->
        </div>
    </div>
</body>
</html>
