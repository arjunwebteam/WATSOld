                    <input type="password" placeholder="Password" onkeypress="ResetValidateClass();" onclick="ResetValidateClass();" name="Password" id="Password1" class="form-control" />
                    <article class="clearfix t-p10 t-c">
                        <input type="submit" value="LOGIN" class="btn btn-danger btn-block poppinssemibold font18 border-radius0" onclick="return LoginValidate();" />
                        <span class="dis-b t-p5">
