            if ($('#CMPassword1').val() == '') { $('#CMPassword1').focus().addClass("validate"); return false; } else { $('#CMPassword1').focus().removeClass("validate"); }


            return true;
        }

        function CuturalMLoginShow() {

            $('#cuturalmainlogin').dialog('open');

        }


