@{
    ViewBag.Title = "Videos";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    var lstVideos = (List<WATS.Entities.Videos>)ViewBag.lstVideos;
    var lstVideoCategories = (List<WATS.Entities.VideoCategories>)ViewBag.lstVideoCategories;
    ViewBag.heading = "Gallery";
}


         <article class="clearfix border t-m1 Poppins-Regular font14 l-h24 black-t" style="min-height:1150px;">
                    <article class="clearfix curve position-r t-m4n b-p15">
                          <article class="clearfix curve position-r t-m4n b-p15">
     <h2 class="Impact font24 black-t l-h16 t-c mobile-font16 b-p15">Video <span class="kaushan_scriptregular font24 red-t l-h14 mobile-font16">Gallery </span></h2></article>
                          
     <article class="clearfix p30 t-p15 tabhorizontal-p10">
         @if (lstVideoCategories != null && lstVideoCategories.Count != 0 && lstVideos.Count != 0)
        {
            foreach (var VideoCategory in lstVideoCategories)
            {
                if (lstVideos != null && lstVideos.Count != 0 && VideoCategory.VideosCount != 0)
                {
     <h2 class="Poppins-Regular merunred-t l-h18 t-b-p10 t-m0">@VideoCategory.CategoryName </h2>
                          }
                           if (lstVideos != null && lstVideos.Count != 0)
                    {
<ul class="clearfix list-pn list-f m0 v-g-list1 black-t tabhorizontal-t-p10 t-c">
     @foreach (var item in lstVideos)
                {
                    if (item.VideoCategoryId == VideoCategory.VideoCategoryId)
                    {
              <li>
                <article class="clearfix overflow-h position-r"> <a href="https://www.youtube.com/embed/@(item.VideoUrl)" data-fancybox-group="gallery" class="various fancybox.iframe dis-b"><span class="clearfix dis-b galleryhover-bg OpenSans-Semibold font11 white-t"><span class="video-icon-hover flip">&nbsp;</span> </span> <img alt="@item.Heading" title="@item.Heading" src="https://i1.ytimg.com/vi/@(item.VideoUrl)/hqdefault.jpg" class="width100" height="160" border="0" width="280"> </a></article>
                  <h5 class="font15 black-t t-m5 b-m0 l-h22">@item.Heading (@item.UpdatedTime.ToString("dd-MMM-yyyy"))</h5>
                
              </li>
     }
     }


            </ul>
               }
                          }
                          }
         else
         {
               <h4 class="clearfix kaushan_scriptregular font24 merunred-t t-c">No Videos Found.</h4>
          }
</article>
         

         </article>
             
         </article>
   
