        if ($('#State').val() == "") { $('#State').focus().addClass("validate"); return false; } else { $('#State').focus().removeClass("validate"); }
        if ($('#people1').val() == '' && $('#people2').val() == '' && $('#people3').val() == '' && $('#people4').val() == '' && $('#people5').val() == '') {
            alert("Please select atleast one Registration Category .");
            returnvalue = false;
        }
        if (!RegValidate()) {
            return true;
        }
        if ($('#txtCaptcha').val() == '') { $('#txtCaptcha').focus().addClass("validate"); return false; } else { $('#txtCaptcha').focus().removeClass("validate"); }
        var str = Common.GetCaptcha();

        if ($('#txtCaptcha').val() != str) { $('#txtCaptcha').val(''); document.getElementsByName('txtCaptcha')[0].placeholder = 'You entered wrong captcha.'; $('#txtCaptcha').focus().addClass("validate"); return false; } else { $('#txtCaptcha').focus().removeClass("validate"); }

        return true;
    }
    function RegValidate() {
        if ('@Memberdetails.MemberId' != "0") {
            if ($('#people1').val() > 10) { $('#people1').val(''); document.getElementsByName('VIPCount')[0].placeholder = 'Max 10'; $('#people1').focus().addClass("validate"); return false; } else { $('#people1').removeClass("validate"); }
            if ($('#people2').val() > 10) { $('#people2').val(''); document.getElementsByName('FamilyCount')[0].placeholder = 'Max 10'; $('#people2').focus().addClass("validate"); return false; } else { $('#people2').removeClass("validate"); }
