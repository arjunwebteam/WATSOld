        if (!Common.CheckEmail($('#EmailId').val())) { $('#EmailId').val(''); document.getElementsByName('EmailId')[0].placeholder = 'Invalid email address'; $('#EmailId').focus().addClass("validate"); return false; } else { $('#EmailId').removeClass("validate"); }
        if ($('#Mobile').val() == "") { $('#Mobile').focus().addClass("validate"); return false; } else { $('#Mobile').focus().removeClass("validate"); }
