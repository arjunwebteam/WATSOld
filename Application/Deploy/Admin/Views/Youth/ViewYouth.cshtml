@{
    ViewBag.Title = "ViewYouth";
    Layout = null;

    WATS.Entities.YouthUserInfo objYouthUserInfo = ViewBag.objYouthUserInfo;
}
@{var baseurl = System.Configuration.ConfigurationManager.AppSettings["baseurl"];}
<base href="@(baseurl)" />
<link href="Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link rel="stylesheet" type="text/css" href="Content/jquery-ui.css" />
<link href="Content/admin-style.css" rel="stylesheet" media="screen">
<link href="Content/admin-comman-styles.css" rel="stylesheet" media="screen">
<link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700' rel='stylesheet' type='text/css'>
<div class="custom-adn grayBackground nobL nobR">
    <div class="ui-accordion ui-widget ui-helper-reset" role="tablist">
        <h3 class="ui-accordion-header ui-helper-reset ui-state-default ui-accordion-header-active ui-state-active ui-corner-top ui-accordion-icons" role="tab" id="ui-accordion-2-header-0" aria-controls="ui-accordion-2-panel-0" aria-selected="true" tabindex="0"><span class="ui-accordion-header-icon ui-icon ui-icon-triangle-1-s"></span><b>Youth Registration Details</b></h3>
        <div class="form-horizontal ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom ui-accordion-content-active" style="display: block;" id="ui-accordion-2-panel-0" aria-labelledby="ui-accordion-2-header-0" role="tabpanel" aria-expanded="true" aria-hidden="false">
            <div class="filedsetInner clearfix">
                <table width="95%" cellspacing="0" cellpadding="0" border="0" align="center" class="main-text">
                    <tbody>
                        <tr>
                            <td>
                                <table width="100%" cellspacing="0" cellpadding="5" border="0">
                                    <tbody>
                                        <tr>
                                            <td width="265">First Name:</td>
                                            <td>@objYouthUserInfo.FirstName</td>
                                        </tr>
                                        <tr>
                                            <td>Last Name :</td>
                                            <td>@objYouthUserInfo.LastName</td>
                                        </tr>
                                        <tr>
                                            <td>Age :</td>
                                            <td>@objYouthUserInfo.Age</td>
                                        </tr>
                                        <tr>
                                            <td>Grade :</td>
                                            <td>@(objYouthUserInfo.Grade == "N/A" ? "" : objYouthUserInfo.Grade)</td>
                                        </tr>
                                        <tr>
                                            <td>Email :</td>
                                            <td>@objYouthUserInfo.Email</td>
                                        </tr>
                                        <tr>
                                            <td>Phone :</td>
                                            <td>@objYouthUserInfo.Mobile</td>
                                        </tr>
                                        <tr>
                                            <td>Is WATS member :</td>
                                            <td>@(objYouthUserInfo.IsMember == true ? "Yes" : "No")</td>
                                        </tr>

                                        <tr>
                                            <td>Hobbies &amp; Interest  :</td>
                                            <td>@(objYouthUserInfo.Hobbies == "" ? "N/A" : objYouthUserInfo.Hobbies)</td>
                                        </tr>
                                        <tr>
                                            <td>Why become  Youth Committee Member :</td>
                                            <td>@(objYouthUserInfo.WhyCommitteeMember == "" ? "N/A" : objYouthUserInfo.WhyCommitteeMember)</td>
                                        </tr>
                                        <tr>
                                            <td>Admin Comments :</td>
                                            <td>@(objYouthUserInfo.AdminComment == "" ? "N/A" : objYouthUserInfo.AdminComment)</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
